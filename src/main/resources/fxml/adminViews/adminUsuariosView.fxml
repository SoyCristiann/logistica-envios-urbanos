<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.uniquindio.pgII.logistica.controlador.adminController.UsuariosAdminController">
    <children>
        <TabPane prefHeight="800.0" prefWidth="1200.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>

                <Tab fx:id="tabAdminUsuario" closable="false" text="Administrar Usuarios">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="763.0" prefWidth="1200.0">
                                    <items>
                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                                            <children>
                                                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#d7d7d7" height="275.0" layoutY="49.0" opacity="0.5" stroke="WHITE" strokeType="INSIDE" strokeWidth="0.0" width="1200.0" />
                                                <VBox alignment="CENTER" prefHeight="378.0" prefWidth="1200.0" spacing="30.0">
                                                    <children>
                                                        <Label alignment="CENTER" contentDisplay="CENTER" text="Administrador de Usuarios" textAlignment="CENTER">
                                                            <font>
                                                                <Font name="System Bold" size="20.0" />
                                                            </font>
                                                        </Label>
                                                        <GridPane alignment="CENTER" hgap="5.0" vgap="20.0">
                                                            <columnConstraints>
                                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                                            </columnConstraints>
                                                            <rowConstraints>
                                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                            </rowConstraints>
                                                            <children>
                                                                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="21.0" prefWidth="587.0" text="Identificación" textAlignment="CENTER">
                                                                    <opaqueInsets>
                                                                        <Insets />
                                                                    </opaqueInsets></Label>
                                                                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="21.0" prefWidth="584.0" text="Nombre Completo" GridPane.rowIndex="1" />
                                                                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="21.0" prefWidth="586.0" text="Email" GridPane.rowIndex="2" />
                                                                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="21.0" prefWidth="585.0" text="Teléfono" GridPane.rowIndex="3" />
                                                                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="21.0" prefWidth="585.0" text="Rol" GridPane.rowIndex="4" />
                                                                <TextField fx:id="inputIdUser" GridPane.columnIndex="1" />
                                                                <TextField fx:id="inputNombreUser" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                                                <TextField fx:id="inputEmailUser" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                                                <TextField fx:id="inputTelefonoUser" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                                                                <ComboBox fx:id="comboRolUser" prefHeight="31.0" prefWidth="592.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                                                            </children>
                                                        </GridPane>
                                                        <HBox alignment="CENTER" spacing="100.0">
                                                            <children>
                                                                <Button fx:id="btnActualizarUser" mnemonicParsing="false" onAction="#actualizarUsuario" text="Actualizar" />
                                                                <Button fx:id="btnCrearUser" mnemonicParsing="false" onAction="#crearUsuario" text="Crear" />
                                                                <Button fx:id="btnLimpiarFormularioUser" mnemonicParsing="false" onAction="#limpiarFormularioUsuario" text="Limpiar formulario" />
                                                                <Button fx:id="btnEliminarUser" mnemonicParsing="false" onAction="#eliminarUsuario" text="Eliminar" />
                                                            </children>
                                                        </HBox>
                                                    </children>
                                                    <padding>
                                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                                    </padding>
                                                </VBox>
                                            </children>
                                        </AnchorPane>
                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="356.0" prefWidth="1198.0">
                                            <children>
                                                <VBox alignment="CENTER" spacing="5.0">
                                                    <children>
                                                        <TableView fx:id="tblUsuarios" prefHeight="328.0" prefWidth="1200.0">
                                                            <columns>
                                                                <TableColumn fx:id="colIdUser" prefWidth="75.0" text="Identificación" />
                                                                <TableColumn fx:id="colNombreUser" prefWidth="75.0" text="Nombre" />
                                                                <TableColumn fx:id="colEmailUser" prefWidth="75.0" text="Email" />
                                                                <TableColumn fx:id="colTelefonoUser" prefWidth="75.0" text="Teléfono" />
                                                                <TableColumn fx:id="colRolUser" prefWidth="304.0" text="Rol" />
                                                            </columns>
                                                            <columnResizePolicy>
                                                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                                            </columnResizePolicy>
                                                            <VBox.margin>
                                                                <Insets />
                                                            </VBox.margin>
                                                        </TableView>
                                                        <HBox alignment="CENTER">
                                                            <children>
                                                                <Label fx:id="lblMensaje" alignment="CENTER" contentDisplay="CENTER" prefHeight="21.0" prefWidth="550.0" text="Label" textAlignment="CENTER" textFill="RED" visible="false">
                                                                    <font>
                                                                        <Font name="System Bold" size="15.0" />
                                                                    </font>
                                                                </Label>
                                                            </children>
                                                        </HBox>
                                                    </children>
                                                </VBox>
                                            </children>
                                        </AnchorPane>
                                    </items>
                                </SplitPane>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>

                <Tab fx:id="tabAdminRepartidores" closable="false" text="Administrar Repartidores">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <fx:include source="adminRepartidoresView.fxml" />
                        </AnchorPane>
                    </content>
                </Tab>

                <Tab fx:id="tabAdminEnvios" closable="false" text="Envíos">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <fx:include source="adminEnviosView.fxml" />
                        </AnchorPane>
                    </content>
                </Tab>

                <Tab fx:id="tabAdminMetricas" closable="false" text="Metricas">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <fx:include source="adminMetricasView.fxml"/>
                        </AnchorPane>
                    </content>
                </Tab>

                <Tab fx:id="tabPerfilMetricas" closable="false" text="Mi Perfil">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <fx:include source="adminPerfilView.fxml"/>
                        </AnchorPane>
                    </content>
                </Tab>

            </tabs>
        </TabPane>
    </children>
</AnchorPane>